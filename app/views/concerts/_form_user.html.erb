<% if user_signed_in? %>
  <ul class="user">
 	<li><%= current_user.email %></li>
 	<li><%= link_to I18n.t("user.out"), destroy_user_session_path, method: :delete %></li>
 <% else %>
 	<li><%= link_to I18n.t("user.reg"), new_user_registration_path %></li>
 	<li><%= link_to I18n.t("user.in"), new_user_session_path %></li>
  </ul>
<% end %>
